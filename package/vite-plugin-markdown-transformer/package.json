{
  "name": "@ainuo-utils/vite-plugin-markdown-transformer",
  "version": "0.0.4",
  "main": "dist/index.js",
  "description": "vite-plugin that transform markdown to html",
  "keywords": [
    "markdown",
    "vue",
    "vite-plugin"
  ],
  "scripts": {
    "build": "rimraf dist && npm run build-bundle",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --external:vite --outfile=dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ainuo5213/vite-plugin.git"
  },
  "readme": "./README.md",
  "dependencies": {
    "markdown-it": "^13.0.1",
    "vite": "^4.2.1",
    "@types/markdown-it": "^12.2.3"
  },
  "devDependencies": {
    "esbuild": "^0.17.16",
    "rimraf": "^5.0.0"
  }
}
